{
  "name": "coding-challenges-in-ts",
  "version": "0.0.0",
  "description": "A repo for code practice and challenges in TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/JAKimball/coding-challenges-in-ts"
  },
  "license": "MIT",
  "author": {
    "name": "Jonathan Kimball",
    "email": "new.cat5598@jonathankimball.dev"
  },
  "type": "module",
  "main": "./lib/index.js",
  "files": [
    "bin/",
    "lib/",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "devtest": "vitest --ui --open --api.port 32250",
    "format": "prettier \"**/*\" --ignore-unknown",
    "format:write": "pnpm format --write",
    "lint": "eslint . .*js --max-warnings 0 --report-unused-disable-directives",
    "lint:knip": "knip",
    "lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
    "lint:package": "npmPkgJsonLint .",
    "lint:packages": "pnpm dedupe --check",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "prepare": "husky",
    "should-semantic-release": "should-semantic-release --verbose",
    "test": "vitest"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.16.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "console-fail-test": "^0.4.4",
    "cspell": "^8.19.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-yml": "^1.18.0",
    "husky": "^9.1.7",
    "jsonc-eslint-parser": "^2.4.0",
    "knip": "^5.62.0",
    "lint-staged": "^15.5.2",
    "markdownlint": "^0.34.0",
    "markdownlint-cli": "^0.41.0",
    "npm-package-json-lint": "^8.0.0",
    "npm-package-json-lint-config-default": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "release-it": "^17.11.0",
    "sentences-per-line": "^0.2.2",
    "should-semantic-release": "^0.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "yaml-eslint-parser": "^1.3.0"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "provenance": true
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "oxc-resolver"
    ]
  }
}
